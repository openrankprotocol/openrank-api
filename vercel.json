{
  "version": 2,
  "rewrites": [
    { "source": "/discord", "destination": "/api/discord" },
    { "source": "/github", "destination": "/api/github" },
    { "source": "/telegram", "destination": "/api/telegram" },
    { "source": "/x", "destination": "/api/x" },
    {
      "source": "/discord/(.*)",
      "destination": "/api/discord/[...params]?params=$1"
    },
    {
      "source": "/github/(.*)",
      "destination": "/api/github/[...params]?params=$1"
    },
    {
      "source": "/telegram/(.*)",
      "destination": "/api/telegram/[...params]?params=$1"
    },
    { "source": "/x/(.*)", "destination": "/api/x/[...params]?params=$1" }
  ],
  "devCommand": "node server.js"
}
